  "scripts": {
    "upload": "scp -i keys/cloud.key -r dist ubuntu@208.113.133.24:project && scp -i keys/cloud.key -r .env ubuntu@208.113.133.24:project",
    "server": "ssh -i keys/cloud.key ubuntu@208.113.133.24",
    "start": "NODE_ENV=production node dist/server/env.js",
    "dev": "concurrently --kill-others \"nodemon src/server/\" \"npm run webpack-dev\" ",
    "flow": "flow check",
    "prettier": "prettier --write './src/**/*.js' --single-quote",
    "build": "npm run webpack-build && babel ./src/server -d ./dist/server && babel ./src/shared -d ./dist/shared",
    "webpack-dev": "cross-env NODE_ENV=development webpack --progress --hide-modules --watch",
    "webpack-build": "cross-env NODE_ENV=production webpack --progress --hide-modules"
