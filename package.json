{
  "name": "universal-javascript-boilerplate",
  "version": "1.2.0",
  "main": "dist/index.js",
  "author": "Evan M Jones <evanjones4040@gmail.com> (https://evansapps.com/)",
  "license": "MIT",
  "engines": {
    "node": "7.6.0",
    "npm": "4.1.2"
  },
  "scripts": {
    "ts-client": "cross-env NODE_ENV=production rollup -c",
    "ts-client-dev": "cross-env NODE_ENV=development rollup -c -w",
    "ts-server": "tsc -p ./tsconfig.server.json",
    "ts-server-dev": "tsc -p ./tsconfig.server.json -w",
    "dev": "concurrently --kill-others \" npm run ts-client-dev\" \" npm run ts-server-dev\" \"NODE_ENV=development nodemon dist/server/entry.js\" ",
    "build": "npm run ts-client && npm run ts-server",
    "start": "NODE_ENV=production node dist/server/entry.js"
  },
  "dependencies": {
    "@types/node": "^7.0.5",
    "background-image-fade-in": "^0.4.4",
    "compression-webpack-plugin": "^0.3.2",
    "concurrently": "^3.3.0",
    "cross-env": "^3.1.4",
    "css-loader": "^0.26.1",
    "empty": "^0.10.1",
    "express": "^4.14.1",
    "extract-text-webpack-plugin": "^2.0.0-beta",
    "gulp": "^3.9.1",
    "node-sass": "^4.5.0",
    "nodemon": "^1.11.0",
    "portfinder": "^1.0.13",
    "prettier": "^0.17.1",
    "pug": "^2.0.0-beta11",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-redux": "^5.0.2",
    "react-router": "^3.0.2",
    "redux": "^3.6.0",
    "rollup": "^0.41.4",
    "rollup-plugin-commonjs": "^7.0.0",
    "rollup-plugin-node-resolve": "^2.0.0",
    "rollup-plugin-replace": "^1.1.1",
    "rollup-plugin-sass": "^0.4.9",
    "rollup-plugin-typescript": "^0.8.1",
    "rollup-plugin-uglify": "^1.0.1",
    "rollup-watch": "^3.2.2",
    "router": "^1.1.5",
    "rxjs": "^5.1.0",
    "sass-loader": "^5.0.1",
    "style-loader": "^0.13.1",
    "ts-loader": "^2.0.0",
    "typescript": "^2.2.1",
    "webpack": "^2.2.1",
    "webpack-node-externals": "^1.5.4"
  }
}
